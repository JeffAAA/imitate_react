{
    "presets": [
        [
            "es2015",
            {
                "modules": false
            }
        ],
        "stage-2",
        "react"
    ],
    "plugins": [
        "transform-runtime",
        [
            "transform-react-jsx",
            {
                "pragma": "React.createElement"
            }
        ],
        "external-helpers",
        "transform-flow-strip-types",
        "syntax-flow",
        "transform-es2015-modules-commonjs"
    ]
}